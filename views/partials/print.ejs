<script>
  const doPdf = (el, name) => {
    const element = document.getElementById(`${el}`);
    const opt = {
      margin: 1,
      filename: `${name}`,
      image: { type: 'jpeg', quality: 0.98 },
      html2canvas: { scale: 2 },
      jsPDF: { unit: 'in', format: 'letter', orientation: 'portrait' },
    };

    html2pdf().from(element).set(opt).save();

    html2pdf(element, opt);
  };
</script>
