<!--  Extra Large modal example -->
<div
  class="modal fade editCartographe"
  tabindex="-1"
  role="dialog"
  aria-labelledby="myExtraLargeModalLabel"
  aria-hidden="true"
>
  <div class="modal-dialog modal-xl modal-dialog-centered">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title mt-0" id="myExtraLargeModalLabel">
          Edit cartographe
        </h5>
        <button
          type="button"
          class="btn-close"
          data-bs-dismiss="modal"
          aria-label="Close"
        ></button>
      </div>
      <div class="modal-body">
        <div class="row">
          <div class="col-lg-12" style="padding: 10px 40px">
            <div
              class="alert alert-success alert-dismissible fade show hidden"
              role="alert"
              id="update"
            >
              <button
                type="button"
                class="btn-close"
                data-bs-dismiss="alert"
                aria-label="Close"
              ></button>
              <strong>Updated with success !</strong>
            </div>
            <form id="form">
              <!-- First Name -->
              <div class="mb-3 row">
                <label for="example-text-input" class="col-md-2 col-form-label"
                  >Nom</label
                >
                <div class="col-md-10">
                  <input
                    class="form-control"
                    type="text"
                    placeholder="Luccin"
                    name="firstName"
                    id="firstName"
                  />
                </div>
              </div>
              <!-- Last Name -->
              <div class="mb-3 row">
                <label for="example-text-input" class="col-md-2 col-form-label"
                  >Postnom</label
                >
                <div class="col-md-10">
                  <input
                    class="form-control"
                    name="lastName"
                    type="text"
                    placeholder="Masirika"
                    id="lastName"
                  />
                </div>
              </div>

              <!-- Email -->
              <div class="mb-3 row">
                <label for="example-text-input" class="col-md-2 col-form-label"
                  >Email</label
                >
                <div class="col-md-10">
                  <input
                    class="form-control"
                    name="email"
                    type="email"
                    placeholder="Email"
                    id="email"
                  />
                </div>
              </div>

              <!-- Phone number -->
              <!-- Airtel number -->
              <div class="mb-3 row">
                <label for="example-tel-input" class="col-md-2 col-form-label"
                  >Airtel</label
                >
                <div class="col-md-10">
                  <input
                    class="form-control"
                    name="airtel"
                    type="tel"
                    placeholder="+243825494121"
                    id="airtel"
                  />
                </div>
              </div>

              <div class="mb-3 row">
                <label
                  for="example-tel-input"
                  class="col-md-2 col-form-label"
                ></label>
                <div class="col-md-10">
                  <div class="form-check form-switch mb-3" dir="ltr">
                    <input
                      type="checkbox"
                      class="form-check-input"
                      name="airtelMoney"
                      value="Yes"
                      id="airtelMoney"
                      checked
                    />
                    <label class="form-check-label" for="customSwitchsizesm"
                      >Airtel Money</label
                    >
                  </div>
                </div>
              </div>

              <!-- Vodacom number -->
              <div class="mb-3 row">
                <label for="example-tel-input" class="col-md-2 col-form-label"
                  >Vodacom</label
                >
                <div class="col-md-10">
                  <input
                    class="form-control"
                    name="vodacom"
                    type="tel"
                    placeholder="+243825494121"
                    id="vodacom"
                  />
                </div>
              </div>

              <div class="mb-3 row">
                <label
                  for="example-tel-input"
                  class="col-md-2 col-form-label"
                ></label>
                <div class="col-md-10">
                  <div class="form-check form-switch mb-3" dir="ltr">
                    <input
                      type="checkbox"
                      class="form-check-input"
                      name="mPesa"
                      value="Yes"
                      id="mPesa"
                      checked
                    />
                    <label class="form-check-label" for="customSwitchsizesm"
                      >M-Pesa</label
                    >
                  </div>
                </div>
              </div>

              <!-- Submit-->
              <div class="mb-3 row">
                <label
                  for="example-text-input"
                  class="col-md-2 col-form-label"
                ></label>
                <div class="col-md-10">
                  <button type="submit" class="btn btn-primary w-md">
                    Submit
                  </button>
                </div>
              </div>
            </form>
          </div>
        </div>
        <!-- end row -->
      </div>
    </div>
    <!-- /.modal-content -->
  </div>
  <!-- /.modal-dialog -->
</div>
<!-- /.modal -->

